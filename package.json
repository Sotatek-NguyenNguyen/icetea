{
  "name": "icetea",
  "version": "1.0.0",
  "description": "Tendermint-based icetea",
  "main": "server.js",
  "scripts": {
    "test": "standard \"web/**/*.js\" \"icetea/**/*.js\" \"test/**/*.js\"",
    "fix": "standard \"web/**/*.js\" \"icetea/**/*.js\" \"test/**/*.js\" --fix",
    "jest": "jest",
    "start": "node ./scripts/start",
    "dev": "tendermint node & npm start & webpack-dev-server --content-base web --inline --hot --port 3001 --open",
    "dev2": "tendermint node & npm start & webpack-dev-server --content-base web --inline --hot --port 3001",
    "dev3": "npm start & webpack-dev-server --content-base web --inline --hot --port 3001",
    "server": "tendermint node & npm start",
    "app": "webpack-dev-server --content-base web --inline --hot --port 3001 --open",
    "reset": "tendermint unsafe_reset_all & node ./scripts/destroy.js",
    "dump": "node ./scripts/dump.js",
    "show": "sudo lsof -i tcp:26657"
  },
  "keywords": [],
  "author": "Trada Tech",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-private-methods": "7.3.2",
    "@babel/plugin-transform-flow-strip-types": "^7.3.4",
    "@webassemblyjs/ast": "^1.8.4",
    "@webassemblyjs/wasm-parser": "^1.8.4",
    "abci": "5.0.1",
    "ansi_up": "3.0.0",
    "body-parser": "1.18.3",
    "chalk": "^2.4.2",
    "dotenv": "^6.2.0",
    "eosjs-ecc": "4.0.4",
    "express": "4.16.4",
    "halting-problem": "1.0.3",
    "handlebars": "4.0.12",
    "jquery": "3.3.1",
    "json-formatter-js": "2.2.1",
    "level": "^4.0.0",
    "lodash": "4.17.11",
    "merk": "^1.3.4",
    "msgpack-lite": "^0.1.26",
    "node-fetch": "2.3.0",
    "npx": "10.2.0",
    "prismjs": "^1.15.0",
    "webpack-dev-server": "3.1.14",
    "websocket": "^1.0.28",
    "websocket-as-promised": "^0.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "babel-loader": "8.0.5",
    "copy-webpack-plugin": "4.6.0",
    "jest": "^24.1.0",
    "nodemon": "1.18.9",
    "standard": "^12.0.1",
    "webpack": "4.29.3",
    "webpack-cli": "^3.2.0"
  }
}
