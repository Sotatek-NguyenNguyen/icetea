{
  "name": "icetea",
  "version": "0.1.0",
  "description": "Tendermint-based icetea",
  "main": "server.js",
  "scripts": {
    "test": "standard",
    "fix": "standard --fix",
    "jest": "jest",
    "start": "standard --fix && node icetea",
    "app": "standard --fix && webpack-dev-server --open",
    "web": "standard --fix && webpack-dev-server",
    "reset": "tendermint unsafe_reset_all && node ./scripts/destroy.js",
    "dump": "node ./scripts/dump.js",
    "show": "sudo lsof -i tcp:26657"
  },
  "keywords": [
    "icetea",
    "blockchain",
    "tendermint"
  ],
  "author": "Trada Tech",
  "license": "MIT",
  "standard": {
    "ignore": [
      "**/tmp/",
      "**/example/"
    ]
  },
  "pre-commit": [
    "fix",
    "jest"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-private-methods": "7.3.2",
    "@babel/plugin-transform-flow-strip-types": "^7.3.4",
    "@webassemblyjs/ast": "^1.8.5",
    "@webassemblyjs/wasm-parser": "^1.8.5",
    "abci": "5.0.1",
    "ansi_up": "3.0.0",
    "body-parser": "1.18.3",
    "chalk": "^2.4.2",
    "dotenv": "^6.2.0",
    "express": "4.16.4",
    "halting-problem": "1.0.3",
    "handlebars": "4.0.12",
    "icetea-common": "github:TradaTech/icetea-common",
    "icetea-web3": "github:TradaTech/icetea-web3",
    "jquery": "3.3.1",
    "json-formatter-js": "2.2.1",
    "level": "^4.0.0",
    "lodash": "4.17.11",
    "merk": "^1.3.4",
    "npx": "10.2.0",
    "prismjs": "^1.15.0",
    "wasm-metering": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "babel-loader": "8.0.5",
    "copy-webpack-plugin": "4.6.0",
    "jest": "^24.5.0",
    "nodemon": "1.18.9",
    "pre-commit": "^1.2.2",
    "standard": "^12.0.1",
    "webpack": "4.29.3",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
